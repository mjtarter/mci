<div class="container">
  <%= form_for(:dorm, :url => {:action => 'create'}, :html => {:class => 'ptb-50'}) do |f| %>

    <div class="form-group">
      <%= f.label(:name, 'Name *') %>
      <%= f.text_field(:name) %>
    </div>
    <div class="form-group">
      <%= f.label(:lat, 'Lat *') %>
      <%= f.text_field(:lat) %>
    </div>
    <div class="form-group">
      <%= f.label(:lng, 'Lng *') %>
      <%= f.text_field(:lng) %>
    </div>
    <div class="form-group">
      <%= f.label(:location, 'Location *') %>
      <%= f.text_field(:location) %>
    </div>
    <div class="form-group">
      <%= f.label(:dorm_type, 'Type *') %>
      <%= f.select(:dorm_type, ['House', 'Apartment', 'Townhome', 'Dorm']) %>
    </div>
    <div class="form-group">
      <%= f.label(:address, 'Address *') %>
      <%= f.text_field(:address) %>
    </div>
    <div class="form-group">
      <%= f.label(:phone, 'Phone *') %>
      <%= f.text_field(:phone) %>
    </div>
    <div class="form-group">
      <%= f.label(:email, 'Email *') %>
      <%= f.text_field(:email) %>
    </div>
    <div class="form-group">
      <%= f.label(:floors, 'Floors *') %>
      <%= f.select(:floors, 1..25) %>
    </div>
    <div class="form-group">
      <%= f.label(:beds, 'Beds *') %>
      <%= f.text_field(:beds) %>
    </div>
    <div class="form-group">
      <%= f.label(:rooms, 'Rooms *') %>
      <%= f.text_field(:rooms) %>
    </div>
    <div class="form-group">
      <%= f.label(:bathrooms, 'Bathrooms *') %>
      <%= f.text_field(:bathrooms) %>
    </div>
    <div class="form-group">
      <%= f.label(:gender_on_floor, 'Gender On Floor *') %>
      <%= f.text_field(:gender_on_floor) %>
    </div><div class="form-group">
      <%= f.label(:description, 'Description') %>
      <%= f.text_area(:description, :size => "50x5") %>
    </div>
    <div class="form-group">
      <%= f.label(:price, 'Price *') %>
      <%= f.text_field(:price) %>
    </div>
    <div class="form-group">
      <%= f.label(:parking_zone, 'Parking Zone *') %>
      <%= f.text_field(:parking_zone) %>
    </div>
    <div class="form-group">
      <%= f.label(:academic_support_center, 'Academic Support Center *') %>
      <%= f.radio_button(:academic_support_center, true) %>
      <%= label_tag(:academic_support_center_true, "yes") %>
      <%= f.radio_button(:academic_support_center, false) %>
      <%= label_tag(:academic_support_center_false, "no") %>
    </div>
    <div class="form-group">
      <%= f.label(:computer_lab, 'Computer Lab *') %>
      <%= f.radio_button(:computer_lab, true) %>
      <%= label_tag(:computer_lab_true, "yes") %>
      <%= f.radio_button(:computer_lab, false) %>
      <%= label_tag(:computer_lab_false, "no") %>
    </div>
    <div class="form-group">
      <%= f.label(:library, 'Library *') %>
      <%= f.radio_button(:library, true) %>
      <%= label_tag(:library_true, "yes") %>
      <%= f.radio_button(:library, false) %>
      <%= label_tag(:library_false, "no") %>
    </div>
    <div class="form-group">
      <%= f.label(:ud_academic_advising, 'UD Academic Advising *') %>
      <%= f.radio_button(:ud_academic_advising, true) %>
      <%= label_tag(:ud_academic_advising_true, "yes") %>
      <%= f.radio_button(:ud_academic_advising, false) %>
      <%= label_tag(:ud_academic_advising_false, "no") %>
    </div>
    <div class="form-group">
      <%= f.label(:ac, 'AC *') %>
      <%= f.radio_button(:ac, true) %>
      <%= label_tag(:ac_true, "yes") %>
      <%= f.radio_button(:ac, false) %>
      <%= label_tag(:ac_false, "no") %>
    </div>
    <div class="form-group">
      <%= f.label(:carpet_in_room, 'Carpet In Room *') %>
      <%= f.radio_button(:carpet_in_room, true) %>
      <%= label_tag(:carpet_in_room_true, "yes") %>
      <%= f.radio_button(:carpet_in_room, false) %>
      <%= label_tag(:carpet_in_room_false, "no") %>
    </div>
    <div class="form-group">
