      <%= f.radio_button(:music_practice_room, false) %>
      <%= label_tag(:music_practice_room_false, "no") %>
    </div>
    <div class="form-group">
      <%= f.label(:piano, 'Piano *') %>
      <%= f.radio_button(:piano, true) %>
      <%= label_tag(:piano_true, "yes") %>
      <%= f.radio_button(:piano, false) %>
      <%= label_tag(:piano_false, "no") %>
    </div>
    <div class="form-group">
