      <%= label_tag(:game_room_true, "yes") %>
      <%= f.radio_button(:game_room, false) %>
      <%= label_tag(:game_room_false, "no") %>
    </div>
    <div class="form-group">
      <%= f.label(:laundry_on_floor, 'Laundry On Floor *') %>
      <%= f.radio_button(:laundry_on_floor, true) %>
      <%= label_tag(:laundry_on_floor_true, "yes") %>
      <%= f.radio_button(:laundry_on_floor, false) %>
      <%= label_tag(:laundry_on_floor_false, "no") %>
    </div>
    <div class="form-group">
